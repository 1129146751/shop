<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fx.shop.mapper.InfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.fx.shop.entity.Info">
        <id column="id" property="id" />
        <result column="moble" property="moble" />
        <result column="address" property="address" />
        <result column="failure_time" property="failureTime" />
        <result column="home_page" property="homePage" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, moble, address, failure_time, home_page
    </sql>

</mapper>
